<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ListHoroActivity"

>
    <FrameLayout
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:id="@+id/backlist" android:layout_marginTop="1dp" android:layout_marginBottom="1dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="1dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginStart="1dp"
            android:layout_marginEnd="1dp" android:layout_marginRight="1dp" android:background="@drawable/gradient">
        <TableLayout
                android:layout_width="360dp"
                android:layout_height="480dp" android:id="@+id/TL" android:layout_gravity="center"
                android:baselineAligned="false">

            <TableRow android:layout_width="match_parent" android:layout_height="match_parent"


                      android:baselineAligned="false">
                <androidx.cardview.widget.CardView
                        android:layout_width="120dp"
                        android:layout_height="120dp" android:id="@+id/CVAries">
                    <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content" android:layout_gravity="bottom">
                        <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content" android:id="@+id/AT"
                                android:gravity="center"
                                android:text="@string/aries_text"/>
                        <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content" android:id="@+id/AD"
                                android:gravity="center"
                                android:text="@string/date_aries" android:textSize="12sp"/>
                    </LinearLayout>
                    <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="87dp" android:alpha="0.70">

                        <ImageView
                                android:layout_width="55dp"
                                android:layout_height="55dp" app:srcCompat="@drawable/aries__1_"
                                android:id="@+id/imageView" android:scaleType="centerCrop"
                                android:layout_centerInParent="true" android:scrollbarAlwaysDrawHorizontalTrack="false"
                                android:scrollbarAlwaysDrawVerticalTrack="false" android:contentDescription="@string/img_aries"/>
                    </RelativeLayout>
                </androidx.cardview.widget.CardView>
                <androidx.cardview.widget.CardView
                        android:layout_width="120dp"
                        android:layout_height="120dp" android:id="@+id/CVTaurus">
                    <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content" android:layout_gravity="bottom">
                        <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content" android:id="@+id/TT"
                                android:gravity="center"
                                android:text="@string/taurus_text"/>
                        <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content" android:id="@+id/TD"
                                android:gravity="center"
                                android:text="@string/date_taurus" android:textSize="12sp"/>
                    </LinearLayout>
                    <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="87dp" android:alpha="0.70">
                        <ImageView
                                android:layout_width="55dp"
                                android:layout_height="55dp" app:srcCompat="@drawable/taurus"
                                android:id="@+id/imageView3"
                                android:layerType="none" android:layout_centerHorizontal="true"
                                android:layout_centerInParent="true"/>
                    </RelativeLayout>
                </androidx.cardview.widget.CardView>
                <androidx.cardview.widget.CardView
                        android:layout_width="120dp"
                        android:layout_height="120dp" android:id="@+id/CVGemini">
                    <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content" android:layout_gravity="bottom">
                        <TextView
                                android:text="@string/gemini_text"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content" android:id="@+id/GT"
                                android:layout_weight="1" android:gravity="center"/>
                        <TextView
                                android:text="@string/date_gemini"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content" android:id="@+id/GD"
                                android:layout_weight="1" android:gravity="center" android:textSize="12sp"/>
                    </LinearLayout>
                    <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="87dp">
                        <ImageView
                                android:layout_width="55dp"
                                android:layout_height="55dp" app:srcCompat="@drawable/gemini"
                                android:id="@+id/imageView4" android:alpha="0.70"
                                android:layout_centerInParent="true"/>
                    </RelativeLayout>
                </androidx.cardview.widget.CardView>
            </TableRow>
            <TableRow android:layout_width="match_parent" android:layout_height="match_parent"


            >
                <androidx.cardview.widget.CardView
                        android:layout_width="120dp"
                        android:layout_height="120dp" android:id="@+id/CVCancer">
                    <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content" android:layout_gravity="bottom">
                        <TextView
                                android:text="@string/cancer_text"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content" android:id="@+id/CT"
                                android:layout_weight="1" android:gravity="center"/>
                        <TextView
                                android:text="@string/date_cancer"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content" android:id="@+id/CD"
                                android:layout_weight="1" android:gravity="center" android:textSize="12sp"/>
                    </LinearLayout>
                    <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="87dp" android:layout_gravity="center|top">
                        <ImageView
                                android:layout_width="55dp"
                                android:layout_height="55dp" app:srcCompat="@drawable/cancer__1_"
                                android:id="@+id/imageView5" android:alpha="0.70"
                                android:layout_centerInParent="true"/>
                    </RelativeLayout>
                </androidx.cardview.widget.CardView>
                <androidx.cardview.widget.CardView
                        android:layout_width="120dp"
                        android:layout_height="120dp" android:id="@+id/CVLeo">
                    <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content" android:layout_gravity="bottom">
                        <TextView
                                android:text="@string/leo_text"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content" android:id="@+id/LT"
                                android:layout_weight="1" android:gravity="center"/>
                        <TextView
                                android:text="@string/date_leo"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content" android:id="@+id/LD"
                                android:layout_weight="1" android:gravity="center" android:textSize="12sp"/>
                    </LinearLayout>
                    <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="87dp">
                        <ImageView
                                android:layout_width="55dp"
                                android:layout_height="55dp" app:srcCompat="@drawable/leo"
                                android:id="@+id/imageView6"
                                android:layout_centerInParent="true" android:alpha="0.7"
                                android:scaleType="centerCrop"/>
                    </RelativeLayout>
                </androidx.cardview.widget.CardView>
                <androidx.cardview.widget.CardView
                        android:layout_width="120dp"
                        android:layout_height="120dp" android:id="@+id/CVVirgo">
                    <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content" android:layout_gravity="bottom">
                        <TextView
                                android:text="@string/virgo_text"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content" android:id="@+id/VT"
                                android:layout_weight="1" android:gravity="center"/>
                        <TextView
                                android:text="@string/date_virgo"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content" android:id="@+id/VD"
                                android:layout_weight="1" android:gravity="center" android:textSize="12sp"/>
                    </LinearLayout>
                    <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="87dp">
                        <ImageView
                                android:layout_width="55dp"
                                android:layout_height="55dp" app:srcCompat="@drawable/virgo"
                                android:id="@+id/imageView7" android:alpha="0.7"
                                android:layout_centerInParent="true"/>
                    </RelativeLayout>
                </androidx.cardview.widget.CardView>
            </TableRow>
            <TableRow android:layout_width="match_parent" android:layout_height="match_parent"


            >
                <androidx.cardview.widget.CardView
                        android:layout_width="120dp"
                        android:layout_height="120dp" android:id="@+id/CVLibra">
                    <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content" android:layout_gravity="bottom">
                        <TextView
                                android:text="@string/libra_text"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content" android:id="@+id/LibT"
                                android:layout_weight="1" android:gravity="center"/>
                        <TextView
                                android:text="@string/date_libra"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content" android:id="@+id/LibD"
                                android:layout_weight="1" android:gravity="center" android:textSize="12sp"/>
                    </LinearLayout>
                    <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="87dp">
                        <ImageView
                                android:layout_width="55dp"
                                android:layout_height="55dp" app:srcCompat="@drawable/libra"
                                android:id="@+id/imageView8" android:alpha="0.7"
                                android:layout_centerHorizontal="true" android:layout_centerInParent="true"/>
                    </RelativeLayout>
                </androidx.cardview.widget.CardView>
                <androidx.cardview.widget.CardView
                        android:layout_width="120dp"
                        android:layout_height="120dp" android:id="@+id/CVScorpio">
                    <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content" android:layout_gravity="bottom">
                        <TextView
                                android:text="@string/scorpio_text"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content" android:id="@+id/ST"
                                android:layout_weight="1" android:gravity="center"/>
                        <TextView
                                android:text="@string/date_scorpio"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content" android:id="@+id/SD"
                                android:layout_weight="1" android:gravity="center" android:textSize="12sp"/>
                    </LinearLayout>
                    <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="87dp">
                        <ImageView
                                android:layout_width="55dp"
                                android:layout_height="55dp" app:srcCompat="@drawable/scorpio"
                                android:id="@+id/imageView9" android:alpha="0.7"
                                android:layout_centerInParent="true"/>
                    </RelativeLayout>
                </androidx.cardview.widget.CardView>
                <androidx.cardview.widget.CardView
                        android:layout_width="120dp"
                        android:layout_height="120dp" android:id="@+id/CVSagittarius">
                    <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content" android:layout_gravity="bottom">
                        <TextView
                                android:text="@string/sagittarius_text"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content" android:id="@+id/SagT"
                                android:layout_weight="1" android:gravity="center"/>
                        <TextView
                                android:text="@string/date_sagittarius"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content" android:id="@+id/SagD"
                                android:layout_weight="1" android:gravity="center" android:textSize="12sp"/>
                    </LinearLayout>
                    <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="87dp">
                        <ImageView
                                android:layout_width="55dp"
                                android:layout_height="55dp" app:srcCompat="@drawable/sagittarius"
                                android:id="@+id/imageView11" android:alpha="0.7"
                                android:layout_centerHorizontal="true" android:layout_centerInParent="true"/>
                    </RelativeLayout>
                </androidx.cardview.widget.CardView>
            </TableRow>
            <TableRow android:layout_width="match_parent" android:layout_height="match_parent"


            >
                <androidx.cardview.widget.CardView
                        android:layout_width="120dp"
                        android:layout_height="120dp" android:id="@+id/CVCapricorn">
                    <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content" android:layout_gravity="bottom">
                        <TextView
                                android:text="@string/capricorn_text"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content" android:id="@+id/CapT"
                                android:layout_weight="1" android:gravity="center"/>
                        <TextView
                                android:text="@string/date_capricorn"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content" android:id="@+id/CapD"
                                android:layout_weight="1" android:gravity="center" android:textSize="12sp"/>
                    </LinearLayout>
                    <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="87dp">
                        <ImageView
                                android:layout_width="55dp"
                                android:layout_height="55dp" app:srcCompat="@drawable/capricorn"
                                android:id="@+id/imageView12" android:alpha="0.7"
                                android:layout_centerInParent="true"/>
                    </RelativeLayout>
                </androidx.cardview.widget.CardView>
                <androidx.cardview.widget.CardView
                        android:layout_width="120dp"
                        android:layout_height="120dp" android:id="@+id/CVAquarius">
                    <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content" android:layout_gravity="bottom">
                        <TextView
                                android:text="@string/aquarius_text"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content" android:id="@+id/AquaT"
                                android:layout_weight="1" android:gravity="center"/>
                        <TextView
                                android:text="@string/date_aquarius"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content" android:id="@+id/AquaD"
                                android:layout_weight="1" android:gravity="center" android:textSize="12sp"/>
                    </LinearLayout>
                    <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="87dp">
                        <ImageView
                                android:layout_width="55dp"
                                android:layout_height="55dp" app:srcCompat="@drawable/aquarius"
                                android:id="@+id/imageView13" android:alpha="0.7"
                                android:layout_centerInParent="true"/>
                    </RelativeLayout>
                </androidx.cardview.widget.CardView>
                <androidx.cardview.widget.CardView
                        android:layout_width="120dp"
                        android:layout_height="120dp" android:id="@+id/CVPisces">
                    <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content" android:layout_gravity="bottom">
                        <TextView
                                android:text="@string/pisces_text"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content" android:id="@+id/PT"
                                android:layout_weight="1" android:gravity="center"/>
                        <TextView
                                android:text="@string/date_pisces"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content" android:id="@+id/PD"
                                android:layout_weight="1" android:gravity="center" android:textSize="12sp"/>
                    </LinearLayout>
                    <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="87dp">
                        <ImageView
                                android:layout_width="55dp"
                                android:layout_height="55dp" app:srcCompat="@drawable/pisces"
                                android:id="@+id/imageView14" android:alpha="0.7"
                                android:layout_centerHorizontal="true" android:layout_centerInParent="true"/>
                    </RelativeLayout>
                </androidx.cardview.widget.CardView>
            </TableRow>
        </TableLayout>
    </FrameLayout>
</androidx.constraintlayout.widget.ConstraintLayout>